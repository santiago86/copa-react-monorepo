    "watch": "npm run clean && rollup --config rollup.config.js",
    "clean": "rimraf lib",
    "eslint": "eslint config src stories  --ext .js --ext .jsx",
    "prettier": "prettier '{config,src,stories}/**/*.{js,jsx}' --write",
    "stylelint": "stylelint 'src/**/*.scss'"
  },
